<!DOCTYPE html>
<html>
<head>
  <title>NystromBuilders</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="google-site-verification" content="4-z8eyBbtPC4UvHeY3OOc_KXOcqmvlHoooEBnMDcVQo" />

  <!--[if lt IE 9]>
    <%= javascript_include_tag 'customM' %>
   <![endif]-->
</head>

<!-- Body Wrapper -->
<div class="body-wrapper">
  <% if flash.any? %>
    <div class="fixed-top">
      <div class="row">
        <div style="margin-left: 30px;">
          <%= flash_messages do |type, text| %>
            <div class="alert alert-dismissable alert-<%= type == :error ? 'danger' : type %>">
              <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
              <%= text %>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  <% end %>

  <!-- Header -->
    <%= render partial: '/shared/header' %>
  <!-- /Header -->

  <!-- Content -->
  <section id="content">
    <%= yield %>
  </section>
  <!-- / Content -->

  <!-- Footer -->
  <%= render partial: '/shared/footer' %>
  <!-- / Footer -->

</div>
<!-- / Body Wrapper -->
</body>
</html>
